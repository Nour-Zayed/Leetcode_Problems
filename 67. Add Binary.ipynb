    "class Solution(object):\n",
    "    def addBinary(self, a, b):\n",
    "        num1 = int(a, 2)\n",
    "        num2 = int(b, 2)\n",
    "        \n",
    "        decimal = num1 + num2\n",
    "        \n",
    "        sum = bin(decimal)[2:]\n",
    "        \n",
    "        return sum"
   ]
