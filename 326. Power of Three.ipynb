   "source": [
    "class Solution(object):\n",
    "    def isPowerOfThree(self, n):\n",
    "        if n <= 0:\n",
    "            return False\n",
    "        while n % 3 == 0:\n",
    "            n //= 3\n",
    "        return n == 1\n"
   ]
